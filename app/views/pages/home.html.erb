

<div class="mapContainer">
  <div id="map" style='width: 800px; height: 400px;'></div> <br>
  <div class="input-group">
      <input type="text" class="form-control" placeholder="Search for...">
      <span class="input-group-btn">
        <button class="btn btn-default" id="updateButton" type="button">Go!</button>
      </span>
    </div><!-- /input-group -->
</div>



<div class="mainContainer">

		<div class="row">
  <div class="col-md-7 col-md-offset-3">
	<% @test.results.each do |result| %>
	<div class="jumbotron">
		<h2><%= result.display_name %></h2> 

		<%= result.location.city %> <br>
		<%= result.venue.display_name %> <br>
<div class="panel panel-default">
    <div class="panel-heading" role="tab" id="headingOne">
      <h4 class="panel-title">
        <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse<%= result.id %>" aria-expanded="true" aria-controls="collapseOne">
          Artists
        </a>
      </h4>
    </div>
    <div id="collapse<%= result.id %>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
      <div class="panel-body">
        <ul>
			<% result.performances.each do |performance| %>
				<li>
					<%= performance.display_name %>
				</li>

			<% end %>
		</ul>
      </div>
    </div>
  </div>
		
	</div>

<% end %>

		</div>
</div>
</div>